{
  "name": "nationpress",
  "main": "expo-router/entry",
  "version": "1.0.12",
  "scripts": {
    "start": "expo start --offline",
    "start:online": "expo start",
    "start:no-dev": "expo start --no-dev",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "bundle:ios": "mkdir -p ios && NODE_ENV=production npx react-native bundle --platform ios --dev false --entry-file node_modules/expo-router/entry.js --bundle-output ios/main.jsbundle --assets-dest ios --reset-cache || npx expo export --platform ios",
    "build:ios": "cd ios && xcodebuild -workspace NationPress.xcworkspace -scheme NationPress -configuration Debug -sdk iphoneos -derivedDataPath build",
    "build:ios:release": "npx expo run:ios --configuration Release --no-build-cache",
    "build:ios:release:xcode": "npm run bundle:ios && cd ios && xcodebuild -workspace NationPress.xcworkspace -scheme NationPress -configuration Release -sdk iphoneos -derivedDataPath build",
    "build:ios:simulator": "npx expo run:ios --configuration Release --simulator",
    "archive:ios": "npx expo run:ios --configuration Release --no-build-cache && cd ios && xcodebuild -workspace NationPress.xcworkspace -scheme NationPress -configuration Release archive -archivePath ./build/NationPress.xcarchive",
    "web": "expo start --web",
    "lint": "expo lint",
    "prebuild": "npx expo prebuild",
    "prebuild:clean": "npx expo prebuild --clean",
    "prebuild:ios": "npx expo prebuild --platform ios",
    "prebuild:android": "npx expo prebuild --platform android",
    "ensure:android": "./scripts/ensure-android.sh",
    "build:android": "./scripts/ensure-android.sh && cd android && ./gradlew assembleDebug",
    "build:android:release": "./scripts/ensure-android.sh && cd android && ./gradlew assembleRelease",
    "install:android": "${ANDROID_HOME:-$HOME/Library/Android/sdk}/platform-tools/adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "install:android:release": "${ANDROID_HOME:-$HOME/Library/Android/sdk}/platform-tools/adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "build:push": "npm run build:android && npm run install:android",
    "build:push:release": "npm run build:android:release && npm run install:android:release",
    "logcat": "${ANDROID_HOME:-$HOME/Library/Android/sdk}/platform-tools/adb logcat",
    "logcat:clear": "${ANDROID_HOME:-$HOME/Library/Android/sdk}/platform-tools/adb logcat -c",
    "logcat:app": "${ANDROID_HOME:-$HOME/Library/Android/sdk}/platform-tools/adb logcat | grep -i 'nationpress'",
    "logcat:errors": "${ANDROID_HOME:-$HOME/Library/Android/sdk}/platform-tools/adb logcat *:E",
    "log:android": "npx react-native log-android",
    "log:ios": "npx react-native log-ios",
    "log:metro": "npx react-native start --verbose",
    "log:rn": "${ANDROID_HOME:-$HOME/Library/Android/sdk}/platform-tools/adb logcat ReactNative:V ReactNativeJS:V *:S",
    "log:all": "${ANDROID_HOME:-$HOME/Library/Android/sdk}/platform-tools/adb logcat ReactNative:V ReactNativeJS:V *:E | grep -E 'ReactNative|ReactNativeJS|nationpress|OneSignal'",
    "log:full": "${ANDROID_HOME:-$HOME/Library/Android/sdk}/platform-tools/adb logcat | grep -iE '(ReactNative|ReactNativeJS|nationpress|OneSignal|\\[App\\]|\\[LanguageSelection\\]|\\[LanguageSelector\\]|com.nationpressapp)'",
    "log:onesignal": "${ANDROID_HOME:-$HOME/Library/Android/sdk}/platform-tools/adb logcat | grep -iE '(OneSignal|onesignal|pushsubscription|tags|segment)'",
    "log:debug": "${ANDROID_HOME:-$HOME/Library/Android/sdk}/platform-tools/adb logcat *:V | grep -iE '(nationpress|\\[App\\]|OneSignal)'"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.1.3",
    "expo": "~54.0.23",
    "expo-blur": "~14.0.1",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "~14.0.1",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.14",
    "expo-speech": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-tracking-transparency": "~6.0.7",
    "expo-web-browser": "~15.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-autoheight-webview": "^1.6.5",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-mobile-ads": "^16.0.0",
    "react-native-onesignal": "^5.2.14",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-tts": "^4.1.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.21.0",
    "react-native-webview": "^13.16.0",
    "react-native-worklets": "0.5.1",
    "react-native-youtube-iframe": "^2.4.1",
    "sp-react-native-in-app-updates": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
